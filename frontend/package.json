{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{ts,tsx,vue,js}'",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "echarts": "^5.5.1",
    "element-plus": "^2.11.4",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@types/node": "^24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.29.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.93.2",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.7",
    "vue-tsc": "^3.1.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,vue,js,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2022": true
    },
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "sourceType": "module",
      "ecmaVersion": 2022,
      "extraFileExtensions": [
        ".vue"
      ]
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "printWidth": 100,
    "trailingComma": "all"
  }
}
